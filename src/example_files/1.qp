// 1. Example of quip syntax

env console;

println("Hello World");

fn test() {
    let x = 1;
    let y = 2;
    let mut z = 3;

    let p = block {
        let a = 1;
        let b = 2;
        let c = 3;
        a + b + c
    };
    let t = {
        p: x + y + z,
        q: x + y + z,
    };
    z = 4;

    println("x = {x}, y = {y}, z = {z}");
    println("p = {p}, t = {t}");
}

test();


